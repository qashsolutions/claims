{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "outputDirectory": "apps/web/dist",
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "apps/api/dist/**/*"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "has": [{ "type": "host", "value": "www.denali.health" }],
      "dest": "https://denali.health/$1",
      "status": 308
    },
    { "src": "/api/(.*)", "dest": "/api/[[...route]].ts" },
    { "src": "/health", "dest": "/api/[[...route]].ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
