    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@claimscrub/shared": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  }
}
