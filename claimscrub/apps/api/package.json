{
  "name": "@claimscrub/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./trpc": {
      "types": "./dist/trpc/router.d.ts",
      "import": "./dist/trpc/router.js"
    },
    "./trpc/context": {
      "types": "./dist/trpc/context.d.ts",
      "import": "./dist/trpc/context.js"
    }
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsup",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@claimscrub/shared": "workspace:*",
    "@claimscrub/validators": "workspace:*",
    "@hono/node-server": "^1.13.8",
    "@hono/trpc-server": "^0.3.2",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.47.2",
    "@trpc/server": "^11.0.0-rc.682",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "hono": "^4.6.14",
    "pino": "^9.5.0",
    "stripe": "^17.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "prisma": "^5.22.0",
    "tsup": "^8.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5"
  }
}
