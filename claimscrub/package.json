{
  "name": "claimscrub",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Pre-Submission Claim Scrubber PWA - Prevent denials before they happen",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "biome format --write .",
    "check": "biome check .",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "db:generate": "pnpm --filter @claimscrub/api db:generate",
    "db:push": "pnpm --filter @claimscrub/api db:push",
    "db:migrate": "pnpm --filter @claimscrub/api db:migrate",
    "db:studio": "pnpm --filter @claimscrub/api db:studio",
    "storybook": "pnpm --filter @claimscrub/ui storybook",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@hono/trpc-server": "^0.3.2",
    "@prisma/client": "^5.22.0",
    "@trpc/server": "^11.0.0-rc.682",
    "hono": "^4.6.14",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@vercel/node": "^5.5.20",
    "tsup": "^8.5.1",
    "turbo": "^2.3.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  }
}
